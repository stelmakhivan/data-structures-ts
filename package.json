{
  "name": "data-structures-ts",
  "version": "1.0.0",
  "description": "Example of data structures with ts",
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:examples": "tsc --project tsconfig.examples.json --noEmit",
    "build": "tsc",
    "build:examples": "tsc --project tsconfig.examples.json",
    "lint": "eslint src/**/*.ts",
    "eslint:format": "eslint src/**/*.ts --fix",
    "prettier:check": "prettier . -c --log-level=warn",
    "prettier:format": "prettier --log-level=warn --write \"src/**/*.{jsx,js,ts,tsx}\"",
    "examples": "ts-node examples"
  },
  "files": [
    "dist",
    "src",
    "!**/__tests__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [],
  "author": "Ivan Stelmakh <stelmakh.developer@gmail.com> (https://github.com/stelmakhivan)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stelmakhivan/data-structures-ts/issues"
  },
  "homepage": "https://github.com/stelmakhivan/data-structures-ts#readme",
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
